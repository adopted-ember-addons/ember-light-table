{
  "name": "Ember Light Table",
  "description": "",
  "url": "https://github.com/adopted-ember-addons/ember-light-table",
  "options": {
    "enabledEnvironments": ["production"],
    "paths": [
      "addon"
    ],
    "external": {
      "data": [{
        "base": "http://emberjs.com/api/",
        "json": "http://builds.emberjs.com/tags/v2.0.0/ember-docs.json"
      }]
    },
    "themedir": "node_modules/yuidoc-ember-theme",
    "helpers": ["node_modules/yuidoc-ember-theme/helpers/helpers.js"],
    "exclude": "vendor",
    "outdir": "docs",
    "linkNatives": true,
    "quiet": true,
    "parseOnly": false,
    "lint": false,
    "nocode": false
  }
}
